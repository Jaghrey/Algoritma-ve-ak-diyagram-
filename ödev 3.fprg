    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="n, sayac, toplam, i" type="Integer" array="False" size=""/>
            <input variable="n"/>
            <assign variable="sayac" expression="1"/>
            <while expression="sayac &lt;= n">
                <assign variable="toplam" expression="0"/>
                <for variable="i" start="1" end="sayac-1" direction="inc" step="1">
                    <if expression="sayac % i == 0">
                        <then>
                            <assign variable="toplam" expression="toplam + i"/>
                        </then>
                        <else/>
                    </if>
                </for>
                <if expression="toplam == sayac">
                    <then>
                        <output expression="sayac" newline="True"/>
                    </then>
                    <else/>
                </if>
                <assign variable="sayac" expression="sayac + 1"/>
            </while>
        </body>
    </function>
